{
  "name": "chat-with-video",
  "version": "1.2.0",
  "description": "Chat with YouTube videos using AI",
  "type": "module",
  "main": "dist/chat-with-video.js",
  "bin": {
    "chat-with-video": "dist/chat-with-video.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "start": "node dist/chat-with-video.js",
    "build:start": "pnpm build && pnpm start",
    "test:watch": "vitest",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:src": "prettier --write \"src/**/*\"",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "cli",
    "youtube",
    "AI",
    "LLM",
    "chat",
    "video"
  ],
  "author": "Yannick Grenzinger",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@eslint/js": "^9.33.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/copy-paste": "^2.1.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "ink-testing-library": "^4.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.5",
    "@ai-sdk/google": "^2.0.8",
    "@ai-sdk/mistral": "^2.0.9",
    "@ai-sdk/openai": "^2.0.19",
    "@vitest/coverage-v8": "^3.2.4",
    "ai": "^5.0.18",
    "commander": "^14.0.0",
    "copy-paste": "^2.2.0",
    "ink": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.1.1",
    "zod": "^4.0.17"
  }
}
